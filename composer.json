{
    "name": "kryptonpay/kryptonpay-php-sdk",
    "type": "library",
    "description": "Integração com API Krypton Pay em PHP",
    "homepage": "https://kryptonpay.com.br/",
    "keywords": [
        "Krypton Pay",
        "composer",
        "package"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Kryptonpay Sistemas de Pagamento",
        "email": "dev@kryptonpay.com.br"
    }],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "nesbot/carbon": "^2.28",
        "symfony/serializer": "^6.2",
        "symfony/property-access": "^6.2",
        "tightenco/collect": "^9.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "KryptonPay\\": "src/"
        }
    }
}
